<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//hibernate/hibernate Mapping DTD 3.0//EN"
        "classpath://org/hibernate/hibernate-mapping-3.0.dtd"
        [ <!ENTITY hibernate-domain SYSTEM "classpath://com/coreweb/hibernate-domain.hbm.xml"> ]
        >

<hibernate-mapping>

	<class name="com.logia.domain.Tenida" table="Tenida">

		&hibernate-domain;

		<property name="fecha" />
		<property name="balaustre" />
		<property name="balaustrePath" />
		<property name="importeSaco" />

		<many-to-one name="grado" class="com.coreweb.domain.Tipo"
			column="idGrado" lazy="false" />

		<many-to-one name="periodo" class="com.coreweb.domain.Tipo"
			column="idPeriodo" lazy="false" />

		<many-to-one name="tipoTenida" class="com.coreweb.domain.Tipo"
			column="idTipoTenida" lazy="false" />

		<set name="asistencias" table="TenidaAsistencia" inverse="false"
			lazy="false" fetch="join" cascade="all">
			<key>
				<column name="idTenida" not-null="false" />
			</key>
			<one-to-many entity-name="com.logia.domain.TenidaAsistencia"></one-to-many>
		</set>

		<many-to-one name="movimientoHospitalario" class="com.logia.domain.MovimientoHospitalario"
			column="idMovHospitalario" lazy="false" />

	</class>

</hibernate-mapping>
