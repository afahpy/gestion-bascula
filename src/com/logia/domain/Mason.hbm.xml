<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//hibernate/hibernate Mapping DTD 3.0//EN"
        "classpath://org/hibernate/hibernate-mapping-3.0.dtd"
        [ <!ENTITY hibernate-domain SYSTEM "classpath://com/coreweb/hibernate-domain.hbm.xml"> ]
        >

<hibernate-mapping>

	<class name="com.logia.domain.Mason" table="Mason">

		&hibernate-domain;

 
		<property name="nombre" />
 
		<many-to-one name="grado" class="com.coreweb.domain.Tipo"
			column="idGrado" lazy="false" />

		<many-to-one name="cargo" class="com.coreweb.domain.Tipo"
			column="idCargo" lazy="false" />

		<many-to-one name="usuarioSistema" class="com.coreweb.domain.Usuario"
			column="idUsuarioSistema" lazy="false" />

		<set name="capitacionesHH" table="CapitacionHH" inverse="false"
			lazy="false" fetch="join" cascade="all">
			<key>
				<column name="idMason" not-null="false" />
			</key>
			<one-to-many entity-name="com.logia.domain.CapitacionHH"></one-to-many>
		</set>


		<many-to-one name="estado" class="com.coreweb.domain.Tipo"
			column="idEstado" lazy="false" />

		<property name="nombres" />
		<property name="apellidos" />
		<property name="fechaNacimiento" />
		<property name="lugarNacimiento" />
		<property name="nacionalidad" />
		<property name="estadoCivil" />
		<property name="ci" />
		<property name="profesion" />
		<property name="especializacion" />
		<property name="direccionParticular" />
		<property name="telefonoParticular" />
		<property name="celular" />
		<property name="email" />
		<property name="lugarDeTrabajo" />
		<property name="cargoQueOcupa" />
		<property name="direccionLaboral" />
		<property name="telefonoLaboral" />
		<property name="emailLaboral" />
		<property name="tipoDeSangre" />
		<property name="conyugeNombreApellido" />
		<property name="conyugeFechaNacimiento" />
		<property name="conyugeNacionalidad" />
		<property name="conyugeProfesion" />
		<property name="conyugeActividadLaboral" />
		<property name="conyugeCargoQueOcupa" />
		<property name="conyugeTelefonolaboral" />
		<property name="conyugeTipoDeSangre" />
		<property name="datosDeHijos" />
		<property name="fechaIniciacionRegularizacion" />
		<property name="fechaAumentoSalario" />
		<property name="fechaExaltacion" />
		<property name="fechaPlacetYQuite" />
		<property name="fechaFechaSuspension" />
		<property name="fechaExpulsion" />

	</class>

</hibernate-mapping>
