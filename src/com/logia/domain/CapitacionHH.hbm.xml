<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//hibernate/hibernate Mapping DTD 3.0//EN"
        "classpath://org/hibernate/hibernate-mapping-3.0.dtd"
        [ <!ENTITY hibernate-domain SYSTEM "classpath://com/coreweb/hibernate-domain.hbm.xml"> ]
        >

<hibernate-mapping>

	<class name="com.logia.domain.CapitacionHH" table="CapitacionHH">

		&hibernate-domain;

		<property name="montoAnual" />

		<many-to-one name="periodo" class="com.coreweb.domain.Tipo"
			column="idPeriodo" lazy="false" />

		<many-to-one name="estado" class="com.coreweb.domain.Tipo"
			column="idEstado" lazy="false" />

		<many-to-one name="mesInicio" class="com.coreweb.domain.Tipo"
			column="idMesInicio" lazy="false" />

		<set name="movimientosCapitacion" table="MovimientoTesoreria"
			inverse="false" lazy="false" fetch="join" cascade="all">
			<key>
				<column name="idCapitacion" not-null="false" />
			</key>
			<one-to-many entity-name="com.logia.domain.MovimientoTesoreria"></one-to-many>
		</set>

	</class>

</hibernate-mapping>
